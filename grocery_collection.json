		{
			"name": "Get all products",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('Status code is 200', () => {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test('Returns an array', () => {",
							"    const response = pm.response.json();",
							"    pm.expect(response).to.be.an('array');",
							"});",
							"",
							"pm.test('Array contains at least one product', () => {",
							"    const response = pm.response.json();",
							"    pm.expect(response.length).to.be.above(0);",
							"});",
							"",
							"pm.test('Product has all expected properties', () => {",
							"    const response = pm.response.json();",
							"    const product = response[0];",
							"    ",
							"    pm.expect(product).to.have.property('id');",
							"    pm.expect(product).to.have.property('category');",
							"    pm.expect(product).to.have.property('name');",
							"    pm.expect(product).to.have.property('inStock');",
							"});",
							"",
							"pm.test('Product ID is a number', () => {",
							"    const response = pm.response.json();",
							"    const product = response[0];",
							"    ",
							"    pm.expect(product.id).to.be.a('number');",
							"});",
							"",
							"pm.test('Product name is a string', () => {",
							"    const response = pm.response.json();",
							"    const product = response[0];",
							"    ",
							"    pm.expect(product.name).to.be.a('string');",
							"});",
							"",
							"pm.test('Product inStock is a boolean', () => {",
							"    const response = pm.response.json();",
							"    const product = response[0];",
							"    ",
							"    pm.expect(product.inStock).to.be.a('boolean');",
